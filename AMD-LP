<script type="text/javascript">
// Demandbase data fields: company_name, country, title, demandbase_sid, industry, sub_industry, employee_count, primary_sic, street_address, city, companyState, zip, phone, stock_ticker,web_site, fortune_1000, forbes_2000, annual_sales, revenue_range, employee_range
// Below are names of hidden fields, which must match with htmlName of Eloqua form fields, to be generated on form submission.  DO NOT CHANGE ORDER OF ARRAY ELEMENT.  Just rename element if
// necessary.
var frm_fields = new Array('companyName', 'countryName', 'title', 'demandbaseSid', 'industry', 'subIndustry', 'employeeCount', 'primarySic', 'streetAddress', 'city', 'companyState', 'zip', 'phone', 'stockTicker', 'webSite', 'fortune1000', 'forbes2000', 'annualSales', 'revenueRange', 'employeeRange');

// populate form fields with data based on email address
function populateFieldsByEmail(data, form_idx) {
	var oForm = document.forms[form_idx];
	var firstName = oForm["firstName"];

	var lastName = oForm["lastName"];
	
	if (data.first_name != undefined) {			
		if(firstName.value == "" && lastName.value == "") {
			firstName.value = data.first_name;
			lastName.value = data.last_name;
		}
	}

    var company = oForm["company"];
	// if company name is already set, do not change it as well as related fields
	// such as country, state and city since we do not override user input
	if (company.value == "") {
		company.value = data.company_name;		
		populateFieldsByCompany(data, form_idx);
	}
}

function populateFieldsByCompany(data, form_idx) {
		var oForm = document.forms[form_idx];
		oForm["country"].value = data.country;
		oForm["state"].value = data.state;
		oForm["city"].value = data.city;
}


