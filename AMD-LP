<script type="text/javascript">
	var Slider;
	var Panels;
	var current = 0;
	var Tabs;
	var Tab;
	var Tabs2;
	var Tab2;
	var Panels;

 function getByClass(classname) {
    var elArray = [];
    var tmp = Slider.getElementsByTagName("*");
	//alert(classname);
    var regex = new RegExp("(^|\s)" + classname + "(\s|$)");
    for ( var i = 0; i < tmp.length; i++ ) {

        if ( regex.test(tmp[i].className) ) {
            elArray.push(tmp[i]);
        }
    }
	//alert(elArray.length);
    return elArray;
}

 function tabNav(num){
	// alert(myWidth);
	Panels[current].style.display = "none";
	var oldTab = Tab[current];
	$(oldTab).removeClass("current");
	current = num;
	//alert(current);
	Panels[current].style.display = "block";
	//Tab[current].style.display = "block";


	var newTab = Tab[current];
	$(newTab).addClass("current");

}

function start() {
	Slider = document.getElementById('coda-slider-1');
	Panels = getByClass('panel');

	Tabs = document.getElementById('coda-nav-1');
	Tab = Tabs.getElementsByTagName('a');

	for(var i = 0; i < Panels.length; i++){
		if(i != 0){
		Panels[i].style.display = "none";
		}
	}
	
	 if(window.location.hash) {
		 var hash = window.location.hash.substring(1); 
		 tabNav(hash - 1);

	} 
}
		 </script>

	<!-- End JavaScript -->

